/* -- 实线 -- */

.solid {
	border: 2rpx solid rgba(0, 0, 0, 0.15);
}

.solid-top {
	border-top: 2rpx solid rgba(0, 0, 0, 0.15);
}

.solid-right {
	border-right: 2rpx solid rgba(0, 0, 0, 0.15);
}

.solid-bottom {
	border-bottom: 2rpx solid rgba(0, 0, 0, 0.15);
}

.solid-left {
	border-left: 2rpx solid rgba(0, 0, 0, 0.15);
}

.solids {
	border: 8rpx solid #eee;
}

.solids-top {
	border-top: 8rpx solid #eee;
}

.solids-right {
	border-right: 8rpx solid #eee;
}

.solids-bottom {
	border-bottom: 8rpx solid #eee;
}

.solids-left {
	border-left: 8rpx solid #eee;
}

/* -- 虚线 -- */

.dashed {
	border: 2rpx dashed #ddd;
}

.dashed-top {
	border-top: 2rpx dashed #ddd;
}

.dashed-right {
	border-right: 2rpx dashed #ddd;
}

.dashed-bottom {
	border-bottom: 2rpx dashed #ddd;
}

.dashed-left {
	border-left: 2rpx dashed #ddd;
}

.block {
	display: block;
}

/* 单行超出影藏 */
.one-line-hidden{
	overflow: hidden;
	text-overflow:ellipsis;
	white-space: nowrap;
}
/* 多行超出影藏 */
.more-line-hidden{
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
	overflow: hidden;
}


 
