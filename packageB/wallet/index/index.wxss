.record{
  position: absolute;
  display: inline-block;
  width: 160rpx;
  height: 55rpx;
  background: #58BDED;
  border-radius: 25rpx 0rpx 0rpx 25rpx;
  font-size: 22rpx;
  text-align: center;
  line-height: 55rpx;
  color: #fff;
  top:50rpx;
  right: 0rpx;

}
.balance-num{
  margin-top: 50rpx;
  text-align: center;
  width: 100%;
  font-size: 38rpx;
  font-weight: 600;
  color: #fff;
}
.balance-tel{
  text-align: center;
  width: 100%;
  font-size: 32rpx;
  font-weight: 600;
  color: #fff;
  
}
.balance-bt{
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 20rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  letter-spacing: 1rpx;
  border-radius: 45rpx;
  background: linear-gradient(#2EB5EE, #2489DF);
}
.detailed-tel{
  margin-top: 30rpx;
  margin-bottom: 10rpx;
  font-weight: 600;
  font-size: 36rpx;
  color: #333333;
}
.detailed-list{
  background: #fff;
  border-radius: 15rpx;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  margin-top: 20rpx;
  padding: 20rpx;
}
.detailed-name{
  font-size: 26rpx;
  color: #333333;
}
.detailed-time{
  margin-top: 10rpx;
  font-size: 22rpx;
  color: #999999;
}
.detailed-list text{
  position: relative;
  top:20rpx;
  left:  20rpx;
  color: #DC4F4E;
  font-size: 24rpx;
}