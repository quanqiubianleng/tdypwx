/* packageB/library/index/index.wxss */
page{
  background: #F5F5F5;
}
/* 滚动公告开始*/
.notice{
  background: #D7ECF5;
}
.notice-bar{
  height: 55rpx;
  line-height: 55rpx;
  border-radius:10rpx;
}

/* 滚动公告结束*/
.card{
  background: #fff;
  border-radius: 15rpx;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  margin-top: 20rpx;
  padding-top: 25rpx;
}
.personal-head{
  display: inline-block;
  width: 110rpx;
  height: 110rpx;
}
.personal-head image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.personal-name{
  display: inline-block;
  margin-left: 20rpx;
  height: 100%;
}
.user-name{
  font-size: 32rpx;
  font-weight: 600;
  margin-right: 15rpx;
  margin-bottom: 8rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.user-sex{
  padding: 2rpx 15rpx;
  font-size: 22rpx;
  margin-right: 15rpx;
  background: #EAF7FC;
  border-radius: 8rpx;
  color: #66BFE6;
}
.user-type{
  padding: 2rpx 15rpx;
  font-size: 22rpx;
  background: #EAF7FC;
  border-radius: 8rpx;
  color: #66BFE6;
  margin-right: 15rpx;
}
.name-bt{
  margin-top: 10rpx;

}
.name-bt text{
  padding: 3rpx 15rpx;
  background: #32AFE0;
  font-size: 28rpx;
  color: #fff;
  border-radius: 10rpx;
}
.personal-btn{
  height: 100%;
}
.tixian{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  background: #32AFE0;
  color: #fff;
  border-radius: 25rpx;
}
.xiangqing{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  margin-top: 20rpx;
  color: #32AFE0;
  border: 1rpx solid #32AFE0;
  border-radius: 25rpx;
}
.personal-tel{
  font-size: 22rpx;
  margin-top: 10rpx;
  padding-left: 10rpx;
}
.personal-tel text{
  margin-left: 10rpx;
  font-size: 22rpx;
  color: #FB8716;
}
.fund{
  border-top: 1rpx solid #ddd;
  margin-top: 10rpx;
}
.fund-ul{
  height: 100%;
  border-right: 1rpx solid #ddd;
}
.fund-uls{
  border-right: 0rpx solid #ddd;
}
.fund-ul-num{
  margin-top: 10rpx;
  text-align: center;
  font-size: 26rpx;
  color: #DC5150;
  font-weight: 600;
}
.fund-ul-tel{
  margin-top: 10rpx;
  font-size: 24rpx;
  font-weight: 600;
  text-align: center;
  color: #999999;
  margin-bottom: 10rpx;
}

.box{
  background: #fff;
  margin-top: 40rpx;
  padding-bottom: 10rpx;
}
.box-card{
  background: #fff;
  box-shadow: 0 4px 4px 0 #DEEFF7;
}
.box-card-search{
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
  font-weight: 600;
  border-bottom: 1rpx solid #ddd;
}
.box-card-search .box-card-tel{
  width: 90%;
  display: inline-block;
}
.box-card-search{
  width: 10%;
  display: inline-block;
  display:flex;
  flex-direction:row;
  align-items:center;
  justify-content: center;
  vertical-align: middle;
}
.box-card-search image{
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
}
.box-card-level{
  height: 60rpx;

}
.level{
  float: left;
  width: 20%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 22rpx;
  
}
.box-list{
  height: 180rpx;
  background: #fff;
  margin-top: 30rpx;
  /* box-shadow:5px 2px 6px #000; */
  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
}
.box-list-img{
  height: 180rpx;
  padding: 30rpx 15rpx;
}
.box-list-img image{
  width: 120rpx;
  height: 120rpx;
}
.box-desc{
  height: 180rpx;
  padding-left: 10rpx;
}
.box-desc-top{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20rpx;
 
}
.box-desc-top-ji{
  position: relative;
  top:-15rpx;
  border: 1rpx solid #32AFE0;
  color: #32AFE0;
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 50%;
  margin-left: 5rpx;
  font-size: 18rpx;
 
}
.box-desc-bottom{
 margin-bottom: 10rpx;
}
.box-desc-bottom-er{
  text-align: left;
  font-size: 20rpx;
  color: #999999;
}
.box-desc-bottom-er text{
  padding: 4rpx 20rpx;
  float: right;
  border: 1rpx solid #32AFE0;
  color: #32AFE0;
  border-radius: 25rpx;
  margin-right: 15rpx;
}