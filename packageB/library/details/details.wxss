/* packageB/library/details/details.wxss */
page{
  background: #F5F5F5;
}
.card{
  margin-top: 20rpx;
  background: #fff;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  border-radius: 15rpx;
  padding: 25rpx 15rpx;
}
.heom{
  text-align: center;
}
.heom image{
  width: 100rpx;
  height: 100rpx;
}
.xinxi{
  height: 100rpx;
}
.name-name{
  font-size: 32rpx;
  font-weight: 600;
  margin-top: 10rpx;
}
.name-sex{
  font-size: 26rpx;
  padding: 5rpx 20rpx;
  background: #EAF7FC;
  color: #32AFE0;
  border-radius: 10rpx;
  font-weight: 600;
  margin-top: 10rpx;
  margin-left: 15rpx;
}
.user-tel{
  font-size: 22rpx;
  color: #333333;
  margin-top: 10rpx;
}
.card-bt{
  height: 100rpx;
  font-size: 28rpx;
  color: #42B9F2;
  line-height: 100rpx;
  text-align: right;
}
.choice{
  height: 80rpx;
  margin-top: 20rpx;
  
}
.choice text{
  padding-left: 15rpx;
  padding-right: 15rpx;
  padding-bottom: 5rpx;
  font-size: 32rpx;
  margin-left: 6%;
  font-weight: 600;
  
}
.on{
  border-bottom: 4rpx solid #32AFE0;
  color: #32AFE0;
}
/* 产生收益开始 */
.cards{
  background: #fff;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  padding-bottom: 30rpx;
  border-radius: 25rpx;
}
.cards-top{
  border-bottom: 1rpx solid #ddd;
}
.money{
  height: 120rpx;
  
}
.money-num{
  line-height: 120rpx;
  font-size: 40rpx;
  font-weight: 600;
  color: #DC5251;
}
.money-des{
  line-height: 140rpx;
  font-size: 28rpx;
  color: #666666;
  margin-left: 15rpx;
}
.tiem{
  height: 120rpx;
  line-height: 120rpx;
  text-align: right;
  color: #666666;
  font-size: 30rpx;
}
.tiem-icn{
  position: relative;
  right: 0rpx;
  top:8rpx;
  margin-left: 10rpx;
}
.cards-list{
  height: 120rpx;
  border-bottom: 1rpx solid #ddd;
}
.cards-list-name{
  color: #333333;
  font-size: 26rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
/* .cards-list-name view{
} */
.cards-list-money{
  color: #333333;
  font-size: 26rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  text-align: right;
}
/* 产生收益结束 */
/* 动态 */
.sign{
  margin-top: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 10rpx;
  border-radius: 15rpx;
  background: #fff;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
}
.sign-list{
  height: 60rpx;
}
.sign-list text{
  font-size: 26rpx;
  margin-left: 15rpx;
}
.sign-list .jin{
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  background: #32AFE0;
  width: 80rpx;
  text-align: center;
  margin-left: 10rpx;
  border-radius: 12rpx;
  color: #fff;
  font-size: 26rpx;
}
.tiems{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  text-align: right;
  color: #333333;
}
/* 动态结束 */
.liuyan{
 margin-top: 20rpx;
}
.liuyan-l{
  text-align: left;
}
.liuyan-r{
  text-align: right;
}
.box-box{
  background: #fff;
  margin-top: 5rpx;
}
.box{
  margin-top: 20rpx;
  padding-bottom: 10rpx;
  height: auto;
  background: #fff;
}
.box-img{
  height: 140rpx;
  display:flex;
  flex-direction:row;
  align-items:center;
  justify-content: center;
  vertical-align: middle;
  height: auto;
}
.box-img image{
  width: 170rpx;
  height: 140rpx;
}
/* .box-bill{
  height: 180rpx;
} */
.box-bill-name{
  margin-bottom: 10rpx;
}
.name{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: left;
  font-size: 32rpx;
  font-weight: 600;
}
.num{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: right;
  font-size: 28rpx;
  font-weight: 600;
  color: #F5A932;
}
/* .box-bill-type{
  height: 60rpx;
} */
.type{
  text-align: left;
  color: #666F83;
  font-size: 26rpx;
}
.fei{
  height: 60rpx;
}
.fei view{
  float: right;  
  border-radius: 5rpx;
  padding: 0rpx;

}
.fei view text{
  display: inline-block;
  line-height: 38rpx;
  height: 38rpx;
  font-size: 22rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
  border: 1rpx solid #F5A932;
}
.fei-tel{
  background: #F5A932;
  border: 1rpx solid #F5A932;
  color: #fff;
  height: 38rpx;
  line-height: 38rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
 border-radius: 5rpx 0 rpx 0 5rpx;
}
.tags-box{
  height: 55rpx;
  overflow:hidden;
}
.tags-list{
  margin-right: 5rpx;
}
.details{
  float: right;
  background: #F5A932;
  color: #fff;
  padding: 3rpx 5rpx;
  text-align: center;
 font-size: 22rpx;
 border-radius: 10rpx;
 margin-top: 10rpx;
}
.null{
  margin-top: 20rpx;
}
.null image{
  height: 350rpx;
  width: 100%;
}