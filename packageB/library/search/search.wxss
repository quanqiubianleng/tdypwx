/* packageB/library/search/search.wxss */
page{
  background: #F5F5F5;
}

.search-box {
  background: #fff;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999999;
}

.search {
  height: 100rpx;
  padding: 15rpx 0rpx;
}

.search-input {
  background: #E9E9E9;
  border-radius: 15rpx;
  height: 70rpx;
}

.search-input input {
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  color: #000;
  /* color: #666666; */
  font-size: 28rpx;
}

.search-inc {
  display: inline-block;
  margin-left: 50rpx;
  height: 70rpx;

}

.search-inc image {
  padding: 5rpx;
  background: #E9E9E9;
  border-radius: 50%;
  float: left;
  width: 65rpx;
  height: 65rpx;
}
.list{
  margin-top: 80rpx;
}
.box-list{
  height: 180rpx;
  background: #fff;
  margin-top: 30rpx;
  /* box-shadow:5px 2px 6px #000; */
  box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
}
.box-list-img{
  height: 180rpx;
  padding: 30rpx 15rpx;
}
.box-list-img image{
  width: 120rpx;
  height: 120rpx;
}
.box-desc{
  height: 180rpx;
  padding-left: 10rpx;
}
.box-desc-top{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20rpx;
 
}
.box-desc-top-ji{
  position: relative;
  top:-15rpx;
  border: 1rpx solid #32AFE0;
  color: #32AFE0;
  display: inline-block;
  width: 28rpx;
  height: 28rpx;
  line-height: 26rpx;
  text-align: center;
  border-radius: 50%;
  margin-left: 5rpx;
  font-size: 18rpx;
 
}
.box-desc-bottom{
 margin-bottom: 10rpx;
}
.box-desc-bottom-er{
  text-align: left;
  font-size: 20rpx;
  color: #999999;
}
.box-desc-bottom-er text{
  padding: 4rpx 20rpx;
  float: right;
  border: 1rpx solid #32AFE0;
  color: #32AFE0;
  border-radius: 25rpx;
  margin-right: 15rpx;
}
.null{
  margin-top: 250rpx;
  text-align: center;
  color: #999999;
  padding-bottom: 50rpx;
}
.null image{
  width: 100%;
  height: 380rpx;
}