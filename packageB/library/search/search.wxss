/* packageB/library/search/search.wxss */
page{
  background: #F5F5F5;
}

.search-box {
  margin-top: 20rpx;
}

.search {

  height: 80rpx;
  padding: 15rpx 0rpx;
}

.search-input {
  background: #fff;
  border-radius: 15rpx;
  height: 50rpx;
}

.search-input input {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #000;
  /* color: #666666; */
  font-size: 28rpx;
}

.search-inc {
  display: inline-block;
  margin-left: 50rpx;
  height: 50rpx;

}

.search-inc image {
  padding: 5rpx;
  background: #fff;
  border-radius: 50%;
  float: left;
  width: 45rpx;
  height: 45rpx;
}

.library-list{
  margin-top: 5rpx;
}
.library-list-ul{
  background: #fff;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  border-radius: 10rpx;
  margin-left: 3%;
  margin-bottom: 25rpx;
}
.library-list-uls{
  background-size: 100% 100%;
  background: #fff;
}
.library-list-ul-yao{
  height: 100%;
  display:flex;
  flex-direction:row;
  align-items:center;
  justify-content: center;
  vertical-align: middle;
}
.library-list-ul-yao image{
  width: 100%;
  height: 100rpx;
}
.renyuan{
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: left;
}
.renyuan image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.renyuan text{
  margin-left: 20rpx;
  font-size: 32rpx;
}
.tag{
  height: 80rpx;
  width: 65%;
  float: right;
  margin-top: 10rpx;
}
.tag image{
  width: 100%;
  height: 100%;
}
.tag text{
  position: relative;
  top: -92rpx;
  left: 25%;
  height: 80rpx;
  width: 100%;
  line-height: 65rpx;
  font-size: 26rpx;
  color: #fff;
}  
.state{
  height: 40rpx;
}
.state text{
  display: inline-block;
  width: 80rpx;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  margin-right: 15rpx;
  border-radius: 25rpx;
  border: 1rpx solid #32AFE0;
  color: #32AFE0;
  font-size: 26rpx;
}
.describe{
  font-size: 24rpx;
  text-align: center;
  color: #333333;
  margin-top: 10rpx;
}
.del{
  margin-top: 20rpx;
  border:1rpx solid #32AFE0;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 26rpx;
  color: #32AFE0;
  border-radius: 25rpx;
}
.time{
  margin-top: 20rpx;
  color: #999999;
  font-size: 20rpx;

}