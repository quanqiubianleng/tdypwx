/* packageD/message/type.wxss */
  page{
    width: 100%;
    height:100%;
    background: #F5F5F5;
  }
  .box{
    position: fixed;
    top:0rpx;
    left: 0rpx;
    z-index: 999999;
    background: #F5F5F5;
  }
  .swiper-tab {
    line-height: 80rpx;
    box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
    border-radius: 10rpx;
    background: #fff;
    margin-top: 20rpx;
  }
  .tab-item{
    padding-left: 50rpx;
    padding-right: 50rpx;
    text-align: center;
    font-size: 15px;
    color: #999999;
    font-weight: bold;
    letter-spacing: 2rpx;
  }
  .on {
    color: #2FB9EF;
    border-bottom: 5rpx solid #2FB9EF;
  }
  .swiper {
    background: #fff;
    margin-top: 134rpx;
    height: auto;
  }
 
  .product-list .product-item {
    position: relative;
    width: 100vw;
    box-sizing: border-box;
    z-index: 999;
    margin-top: 0rpx;
    margin-bottom: 10rpx;
    margin-top: 130rpx;
  }
  
  .slide-product-list .slide-product-item {
    position: relative;
    width: 100vw;
    border-bottom: 2rpx solid #E9E9E9;
    box-sizing: border-box;
    background: #fff;
    z-index: 999;
  }
  
  .product-list .product-item movable-area {
    height: 225rpx;
    width: calc(100vw - 360rpx);
  }
  
  .product-list .product-item movable-view {
    height: 225rpx;
    width: 100vw;
    z-index: 999;
  }
  
  .product-list .product-item .delete-btn {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 32rpx; 
    width: 360rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #FFFFFF;
    line-height: 225rpx;
    z-index: 1;

    text-align: center;
  }
  .weidu{
    display: inline-block;
    width: 180rpx;
    height: 100%;
    background: #999999;
  }
  .shanchu{
    display: inline-block;
    width: 180rpx;
    height: 100%;
    background: #E66671;
    border-radius:0 18rpx 18rpx 0;
  }
  .product-list .product-item-wrap {
    position: relative;
    display: flex;
    align-items: center;
    padding: 8rpx 0 20rpx 20rpx;
    box-sizing: border-box;
  }
  
  .product-list .product-item-wrap .product-movable-item {
    flex: 1;
    overflow: hidden;
  }
  
  .product-list .product-item-wrap .product-movable-item-name {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #71747A;
    line-height: 60rpx;
    margin-right: 10rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }
  
  .product-list .product-item-wrap .product-movable-item-code {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #969AA3;
  }
  
  .product-list .product-item-wrap .product-movable-item-amount {
    flex: 0 0 auto;
    padding-right: 80rpx;
    position: relative;
  }
  
  .product-list .product-item-wrap .product-movable-item-amount .amount {
    width: 120rpx;
    font-size: 28rpx;
    color: #383A3D;
    line-height: 60rpx;
  }
  
  .product-list .product-item-wrap .product-movable-item-amount .unit {
    position: absolute;
    top: 0;
    right: 30rpx;
    font-size: 28rpx;
    color: #969AA3;
    line-height: 60rpx;
  }  
  .box-box{
    margin-top: 130rpx;
  }
  .box-list{
    height: 225rpx;
    background: #fff;
    border-radius: 15rpx;
    margin-bottom: 20rpx;
  }
  .list{
    height: 100%;
    background: #fff;
    border-radius: 15rpx;
  }
   .null{
     width: 100%;
     height: 100%;
     text-align: center;
     margin-top: 20rpx;
   }
   .list-top{
     line-height: 80rpx;
     padding: 0 32rpx;
    border-bottom: 1rpx solid #ddd;
   }
   .list-top-tie{
     text-align: left;
     font-size: 30rpx;
     color: #333333;
   }
   .list-top-time{
    text-align: right;
    font-size: 30rpx;
    color: #999999;
   }
   .list-desc{
    padding: 0 32rpx;
   margin-top: 20rpx;
   margin-bottom: 20rpx;
    font-size: 28rpx;
    color: #999999;
   }
   .list-bt{
    padding: 0 32rpx;
   }
   .list-bts{
    padding: 0 32rpx;
   }
   .list-bt view{
     float: right;
    background: #3AB3E1;
    padding: 5rpx 30rpx;
    border-radius: 15rpx;
    font-size: 24rpx;
    color: #fff;
   }
   .xiangxin{
     float: right;
     border: 1rpx solid #3AB3E1;
    padding: 5rpx 30rpx;
    border-radius: 15rpx;
    font-size: 24rpx;
    color: #3AB3E1;
    margin-left: 20rpx;
   }
   .null image{
     width: 100%;
   }