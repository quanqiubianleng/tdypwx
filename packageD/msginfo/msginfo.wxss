/* 推荐工作 */
.recommendJob{
  width: 100%;
  box-sizing: border-box
}
.recommendJob .bigTitle{
  display: block;
  width: 100%;
  font-size: 36rpx;
  font-weight: 600;
  color: #111A34;
  padding: 16rpx 32rpx;
  box-sizing: border-box
}
.recommendJob .screenBar{
  width: 100%;
  padding: 0 32rpx;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  position: sticky;
  top: 0;
  background-color: white;
  z-index: 999
}
.recommendJob .screenBar button{
  width: 25%;
  height: inherit;
  line-height: 88rpx;
  text-align: center;
  padding: 0 10rpx;
  box-sizing: border-box;
  background: white;
  display: flex;
  flex-flow: row nowrap
}
.recommendJob .screenBar button::after{
  border: none
}
.recommendJob .screenBar .icon{
  transform: rotate(90deg)
}
.recommendJob .screenBar button text{
  display: inline-block;
  width: 80%;
  height: inherit;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.recommendJob .screenBar button[disabled] text{
  color: #ccc
}

.recommendJob .screenItem text{
  font-size: 28rpx;
  color: #888888
}

.shadow{
  width: 100vw;
  height: 100vh;
  background: rgba(0,0,0,.5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999999
}
.shadow .popUp{
  padding: 0 40rpx;
  background: white;
  border-radius: 0 0 16rpx 16rpx;
  box-sizing: border-box;
  max-height: 670rpx;
  overflow-y: scroll
}
.popUp .fItem view{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  color: #41485D;
  border-bottom: 1rpx solid #E2E4EA
}
.popUp .fItem view:last-child{
  border: none
}
.active{
  color: #111A34 !important
}
.popUp .jobItem{
  margin: 0 -20rpx
}
.popUp .jobItemList{
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  max-height: 480rpx;
  overflow-y: scroll
}
.jobItemList .listItem{
  display: inline-block;
  width: 25%;
  height: 64rpx;
  padding: 0 16rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx
}
.popUp .jobItemList text{
  display: block;
  width: 100%;
  height: inherit;
  line-height: 64rpx;
  text-align: center;
  background: #F8F9FA;
  border-radius: 4rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 auto;
  border:2rpx solid #F8F9FA
}
.jobItem .groupTitle{
  color: #2E54EB;
  padding: 32rpx 20rpx
}
.jobItem .btnGroup{
  margin: 0 -40rpx;
  height: 88rpx;
  display: flex;
  flex-flow: row nowrap;
  border-top: 1rpx solid #E2E4EA
}
.jobItem .btnGroup view{
  flex: 1;
  height: inherit;
  font-size: 32rpx;
  text-align: center;
  line-height: 88rpx
}
.jobItem .btnGroup view:first-child{
  background: white;
  color: #41485D;
}
.jobItem .btnGroup view:last-child{
  background: #2E54EB;
  color: white;
}
.groupTitle{
