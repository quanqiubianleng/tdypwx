.activity{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  z-index: 9999;
  background: rgba(37, 38, 45, 0.5);
}
.activity .swiperBox{
  width: 100%;
  height: 860rpx;
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-55%);
}
.activity swiper{
  height: 100%;
}
.activity swiper-item{
  width: 100%;
  height: 100%;
  padding: 0 75rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
}
.activity .itemBox{
  width: inherit;
  height: 800rpx;
  background: white;
  border-radius: 30rpx;
  overflow: hidden;
  position: relative;
}
.itemBox image{
  width: 100%;
  height: 100%;
}
.itemBox .oprate{
  width: 100%;
  height: 220rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  bottom: 0;
}
.oprate .desc{
  height: 48rpx;
  line-height: 48rpx;
  font-size: 32rpx;
  color: #111A34;
  text-align: center;
}
.desc text{
  color: #396BFF;
}
.oprate .sign{
  margin-top: 32rpx;
  color: white;
  width:100%;
  height:88rpx;
  line-height: 88rpx;
  background:linear-gradient(180deg,rgba(107,164,255,1) 0%,rgba(57,107,255,1) 100%);
  border-radius:44px;
}
.oprate .sign::after{
  border: none;
}
.activity .close{
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  left: 50%;
  bottom: -80rpx;
  transform: translateX(-50%);
}
.activity .close image{
  width: 100%;
  height: 100%;
}



