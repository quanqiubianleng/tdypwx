.topBar{
  width: 90%;
  height: 88rpx;
  padding: 10vw;
  box-sizing: border-box;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
  background: white;
  background-color: #F5F5F5;  
}
.topBar .position{
  width: 120rpx;
  height: 100%;
  text-align: center;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.topBar .position text{
  font-size: 20rpx;
  width: 100%;
  display: block;
  height: 24rpx;
  line-height: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.topBar .searchBox{
  display: block;
  width: 100%;
  height: 68rpx;
  line-height: 68rpx;
  background-color:#E9E9E9;
  border-radius: 20rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  text-align: center;

  left: 2vw;
  position: relative;

}
.searchBox image{
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.searchBox text{
  color: #666F83;
  font-size: 28rpx;
  padding-left: 10rpx;
}
.topBar .userInfo{
  width: 120rpx;
  height: 100%;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.userInfo .avatarBox{
  display: block;
  margin: 0 auto;
  width: 36rpx;
  height: 36rpx;
  margin: 0 auto 4rpx;
  background-color: white;
  border-radius: 20rpx;
  overflow: hidden;
}
.userInfo text{
  display: block;
  text-align: center;
  font-size: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* banner */
