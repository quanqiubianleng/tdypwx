/* pages/clock/personal/personal.wxss */
page{
  background: #F5F5F5;
}
.top-col{
  height: 300rpx;
  width: 100%;
  background: linear-gradient(to right,#3AB3E1, #4A9BED);
  border-radius: 0rpx 0rpx 70rpx 70rpx;
}
.top-col-lf{
  margin-top: 30rpx;
  color: #fff;
  text-align: left;
}
.top-col-lf-name{
  font-size: 38rpx;
  font-weight: 600;
}
.top-col-rf{
  margin-top: 20rpx;
  color: #fff;
  text-align: right;
}
.top-col-rf-ruz{
 font-size: 32rpx;
 letter-spacing: 1rpx;
  font-weight: 600;
}
.top-col-rf-tian{
  display: inline-block;
  margin-top: 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0rpx 35rpx;
  text-align: center;
  background: #fff;
  color: #3AB3E1;
}
.card{
  margin-top: -100rpx;
  background: #fff;
  border-radius: 20rpx;
}
.card-month{
  color: #3AB3E1;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 40rpx;
  font-weight: 600;
  border-radius: 15rpx 15rpx 0rpx 0rpx;
  background: #F0FBFF;
}
.card-p{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.card-type{
  text-align: center;
}
.card-type-in{
  text-align: center;
  box-sizing: border-box;
}
.card-type-inc image{
  float: left;
  margin-left: 23%;
  width: 100rpx;
  height: 100rpx;
}
.card-type-inc text{
  height: 100rpx;
  line-height: 100rpx;
  color: #3AB3E1;
  font-size: 32rpx;
  font-weight: 600;
}
.card-type-inc-tel{
  margin-top: 15rpx;
  letter-spacing: 1rpx;
  color: #3AB3E1;
  font-size: 32rpx;
  font-weight: 600;
}
.bot-surface{
  margin-top: 50rpx;
  border-radius: 20rpx;
  background: #fff;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
}
/* .bot-surface-inc{
} */
.bot-surface-inc-l{
  height: 80rpx;
  margin-top: 25rpx;
  text-align: right;
}
.bot-surface-inc-l image{
  height: 40rpx;
  width: 50rpx;
}
.bot-surface-inc-c{
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 36rpx;
  font-weight: 600;
  text-align: center;
}
.bot-surface-inc-r{
  height: 80rpx;
  margin-top: 25rpx;
  text-align: left;
}
.bot-surface-inc-r image{
  height: 40rpx;
  width: 50rpx;
}
.bot-surface-quan{
  margin-top: 30rpx;
}
.bot-surface-q{
  float: left;
  width: 80rpx;
  height: 80rpx;
  margin-left: 15rpx;
  border-radius: 50%;
  border: 1rpx solid #3AB3E1;
 margin-bottom: 30rpx;
}
.bot-surface-q-inc{
  text-align: center;
}
.bot-surface-q-ao{
  text-align: center;
  font-size: 22rpx;
  color: #3AB3E1;
  width: 100%;
 margin-top: 8rpx;
}
.bot-surface-q-le{
  text-align: center;
  font-size: 20rpx;
  color: #E87751;
}

