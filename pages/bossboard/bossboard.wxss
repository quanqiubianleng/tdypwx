.iman{
  background-color: #F6F5F8;
  height: 100vh;
}
/*圆环相关start*/
.container,.container1 {
  position: absolute;
  width: 50vw;
  height: 20vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  left: -20rpx;
  top: -2vh;
}
.container1 {
  left: 50vw;
  height: 33vh;
  left: 44vw;
}
 
.progress_box {
  position: absolute;
  width: 100px;
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: transparent;
  top: 80rpx;
}
 
.progress_bg {
  position: absolute;
  width: 100px;
  height: 100px;
}
 
.progress_canvas {
  width: 100px;
  height: 100px;
}
 
.progress_text {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
}
 
.progress_info,.progress_info1 {
  font-size: 35rpx;
  color: #38BBF1;
  position: relative;
}
.progress_info1{
  color: #FA541C;
}
.progress_info2{
  color: #3D6CFE;
}
/*圆环相关end*/

/*卡片start*/
.card {
  border: 2px solid #ffffff;
  border-radius: 5px;
  background-color: #ffffff;
  box-shadow: 2px 1px 4px 4px #cccccc;
  margin: 0 4vw;
  position: relative;
  top: 2vh;
  height: 25vh;
  padding: 0 10vw;
}

