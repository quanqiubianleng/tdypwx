/* pages/face-record/index/index.wxss */
page {
  width: 100%;
  height: 100%;
  background: #F5F5F5;
}

.wraper {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.header {
  position: fixed;
  background: #F5F5F5;
  top: 0;
  left: 0;
  z-index: 999999;
  color: #fff;
  width: 100%;
}

.search-box {
  background: #fff;
}

.search {
  height: 80rpx;
  padding: 15rpx 0rpx;
}

.search-input {
  background: #E9E9E9;
  border-radius: 15rpx;
  height: 50rpx;
}

.search-input input {
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #000;
  /* color: #666666; */
  font-size: 28rpx;
}

.search-inc {
  display: inline-block;
  margin-left: 50rpx;
  height: 50rpx;

}

.search-inc image {
  padding: 5rpx;
  background: #E9E9E9;
  border-radius: 50%;
  float: left;
  width: 45rpx;
  height: 45rpx;
}

 /* banner */
 .banner{
  width: 100%;
  height: 250rpx;
  margin-top: 16rpx;
  margin-bottom: 10rpx;
}
.swiper{
  width: inherit;
  height: inherit;
}
.swiper swiper-item{
  padding: 0 32rpx;
  box-sizing: border-box;
}
.swiper image{
  border-radius: 16rpx;
  overflow: hidden;
  width: inherit;
  height: inherit;
  background-color: #eee
}
/* banner结束 */

.list-tel {
  height: 80rpx;
}
.list-tel-ul{
  height: 60rpx;
  margin-top: 10rpx;
  border-radius: 30rpx;
}
.list-tel-ul-ui{
  background:#2FB9EF ;
  border-radius: 15rpx;
  height: 58rpx;
  margin-right: 10rpx;
  line-height: 58rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
}
.on{
  background:red ;
}
.list-tel-tel{
  color: #333;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  font-weight: 600;
 
}
.main {
  position: relative;
  /* z-index: -22; */
  top: 430rpx;
}

.main-scroll {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}

.card {
  width: 100%;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.card-box {
  width: 100%;
  height: 200rpx;
  border-radius: 6rpx;
  background: #ccc;
}

.card-content {
  color: blue;
}

.list {
  background: #fff;
  border-radius: 15rpx;
  box-shadow: 0 4px 4px 0 rgba(0, 0, 0, 0.2);
  padding: 10rpx 50rpx;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
}

.list-usrename {
  height: 110rpx;
}

.list-usrename .name {
  color: #333;
  font-size: 34rpx;
  font-weight: 600;
  margin-bottom: 15rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap
}

.list-usrename .tel {
  color: #666;
  margin-top: 15rpx;
  font-size: 30rpx;
}

.list-inp {
  height: 110rpx;
  padding-top: 35rpx;
}

.list-bt {
  height: 40rpx;
  width: 150rpx;
  letter-spacing: 1rpx;
  background: #D8E1EF;
  color: #3B67B0;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 25rpx;
}

/* 弹框 */
.popup {
  border-radius: 15rpx;
}

.bottom {
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  height: 100rpx;
}

.popup-text {
  margin-top: 50rpx;
  font-size: 46rpx;
  font-weight: 600;
  text-align: center;
}

.popup-te {
  color: #2FB9EF;
  font-size: 32rpx;
  margin-top: 20rpx;
}

.popup-inp {
  margin-top: 30rpx;
}

.popup-inp textarea {
  width: 85%;
  border: 1rpx solid #666666;
  border-radius: 15rpx;
  padding: 10rpx;
  max-height: 200rpx;
  color: #666666;
  font-size: 26rpx;
}

.quxiao {
  float: left;
  margin-top: 20rpx;
  margin-left: 25%;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #333;
  font-size: 28rpx;
  background: #E5E5E5;
  border-radius: 15rpx;
}

.queding {
  float: right;
  margin-right: 25%;
  margin-top: 20rpx;
  width: 150rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #3AB3E1;
  border-radius: 15rpx;
}

.bg {
  display: none;
  position: absolute;
  top: 0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: black;
  z-index: 1001;
  -moz-opacity: 0.7;
  opacity: 0.70;
  filter: alpha(opacity=70);

}

.show {
  display: none;
  text-align: center;
  position: absolute;
  top: 30%;
  left: 15%;
  width: 70%;
  height: 40%;

  z-index: 1002;
  overflow: auto;
}

.show-diy {
  border-radius: 5%;
  margin-top: 80rpx;
  background: #fff;
  width: 80%;
  height: 80%;
  margin-left: 10%;
}

.show-img {
  position: relative;
  z-index: 1003;
  top: -70rpx;
  left: 3%;
  width: 65%;
  height: 60%;
}

.show-tex {
  margin-top: -20%;
  text-align: center;
}

.show-btn {
  width: 70%;
  margin-top: 50rpx;
  height: 100rpx;
  margin-left: 15%;
  line-height: 100rpx;
  background: #30B9EF;
  letter-spacing: 1rpx;
  color: #fff;
  border-radius: 40rpx;
}

.null {
  height: 300rpx;
  margin-top: 30rpx;
  background-size:100% 100%;
}
.null image{
  width: 100%;
  height: 100%;
}
.footer {

  background: #fff;
  color: #fff;
  line-height: 100rpx;
  flex: 0 0 100rpx;
  /* 不放大不缩小固定100rpx */
}