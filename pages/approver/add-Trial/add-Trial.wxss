/* pages/approver/add-Trial/add-Trial.wxss */
page{
  background: #F5F5F5;
}
.msgGroup{
  background: white;
  margin-top: 16rpx;
}
.msgGroup .msgItem{
  width: 100%;
  /* height: 116rpx; */
  border-bottom: 1rpx solid #eee;
}
.msgItems{
  width: 100%;
  height: 116rpx;
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
  padding: 0 32rpx;
}
.msgGroup .msgItem:last-child{
  border: none
}
.msgGroup .msgItems:last-child{
  border: none
}
.msgItem .inBox{
  width: inherit;
  height: inherit;
  position: relative
}
.inBoxs {
  width: inherit;
  height: inherit;
  position: relative
}

.msgItem .inBox .mainIcon{
  position: absolute;
  width: 68rpx;
  height: 68rpx;
  left: 32rpx;
  top: 50%;
  transform: translateY(-34rpx)
}
 .mainIcons{
  position: absolute;
  width: 68rpx;
  border-radius: 50%;
  height: 68rpx;
  left: 0;
  top: 50%;
  transform: translateY(-34rpx)
}
.msgItem .inBox .iconOpen{
  position: absolute;
  right: 32rpx;
  top: 50%;
  transform: translateY(-50%);
}
.msgItems .inBoxs .iconOpens{
  position: absolute;
  font-size: 28rpx;
  right: 10rpx;
  color: #999999;
  top: 50%;
  transform: translateY(-50%);
}
.msgItem .inBox text{
  line-height: 116rpx;
  padding-left: 126rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.inBoxs text{
  line-height: 116rpx;
  padding-left: 94rpx;
  font-size: 28rpx;
  font-weight: 600;
}
.bottom{
  background:#fff;
  position: fixed;
  z-index: 9999;
  margin-top: 30rpx;
  bottom: 0rpx;
  left: 0rpx;
  height: 100rpx;
  border-top: 1rpx #ddd solid;
}
.bottom-number{
  height: 100rpx;
  line-height: 100rpx;
  text-align: left;
  color: #2FB9EF;
  font-size: 36rpx;
}
.bottom-number text{
  margin-left: 6rpx;
  margin-right: 6rpx;
}
.bottom-bt{
  background: #2FB9EF;
  height: 60rpx;
  margin-top: 20rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  letter-spacing: 1rpx;
  border-radius: 10rpx;
}