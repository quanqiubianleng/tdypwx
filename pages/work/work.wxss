.iman{
  background-color: #F5F5F5;
  height: 100vh;
  padding-bottom: 50rpx;
  overflow-x: hidden;
}
page{
  background: #F8F9FA;
  width: 100%;
  height:100%;
}
 
.wraper{
  display: flex;
  flex-direction: column;
  width: 100%;
  height:100%;
}
 
.header{
  color: #fff;
  line-height: 100rpx;
  flex: 0 0 100rpx; /* 不放大不缩小固定100rpx */
}
 /* banner */
.banner{
  width: 100%;
  height: 20vh;
  /* margin-top: 26rpx; */
}
.swiper{
  width: inherit;
  height: inherit;
}
.swiper swiper-item{
  padding: 0 32rpx;
  box-sizing: border-box;
}
.swiper image{
  border-radius: 16rpx;
  overflow: hidden;
  width: inherit;
  height: inherit;
  background-color: #eee
}
/* banner结束 */

/* 滚动公告开始*/
.notice{
  height: 60rpx;
  margin-top: 15rpx;
}
.notices{
  background: #D7ECF5;
  padding-left: 32rpx;
  height: 50rpx;
  margin-top: 15rpx;
  
}
.swiper_container {
  /* display: inline-block;  */
  float: right;
  text-align: left;
  height: 50rpx;
  line-height: 50rpx;
  width: 90%;
  margin-left: 10rpx;
}
.swiper_items {
font-size: 24rpx;
overflow: hidden;
text-overflow: ellipsis;
white-space: nowrap;
color: #666666;
}
/* 滚动公告结束*/
/* 工作中心 */
.toppos{
  color: #333;
  font-size: 30rpx;
  font-weight: 600;
  margin-bottom: 3rpx;
  margin-top: 30rpx;
}
.main{
  flex: 1;
  position: relative;
}
 
.main-scroll{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
 
/* .main-list{
  
