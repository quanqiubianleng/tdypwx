 /*  导航组*/
.menu{
  background-color: #ffffff;
  box-shadow: 0px 4px 4px 0px #cccccc;
  height: auto;
  border-radius: 15rpx;
  margin-bottom: 20rpx;
}
.menu-type-tetl{
  color: #666;
  font-size: 32rpx;
  margin-top: 15rpx;
}
.menu_container {
  width: 100%;
}

.menu_item {
  width: 25%;
  float: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 12px;
  box-sizing: border-box;
  padding-bottom: 10px;
  padding-top: 10px;
}
.menu_icon {
  width: 45px;
  height: 45px;
  padding-bottom: 5px;
}
.menu_name{
  font-size: 24rpx;
}

 
