/* pages/modify/details/details.wxss */
page{
  width: 100%;
  height: 100%;
  background: #F5F5F5;
}

.list{

  height: auto;
}
.lists{
  height: auto;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.basic{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  font-weight: 600;
  background:#EAF7FF
}
.basics{
  background:#F5F5F5
}
.card{
  background: #fff;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
  padding: 30rpx 32rpx 35rpx 32rpx;
}
.card-list-l{
  color: #999999;
  text-align: left;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}
.card-list-r{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start ;
  color: #333333;
  text-align: left;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}
.card-list-text{
  background: #EDF9FE;
  border-radius: 5rpx;
  padding: 8rpx;
  font-size: 30rpx;
  color: #000;
  padding-bottom: 20rpx;
  text-align: justify;
  text-justify: newspaper;
  word-break: break-all;
  
}
.cards{
  background: #fff;
  border-radius: 15rpx;
  padding-bottom: 120rpx;
  box-shadow: 0 4px 4px 0 rgba(0,0,0,0.2);
}
.cards-tex{
  font-size: 36rpx;
  margin-top: 30rpx;
  font-weight: 600;
  color: #999999;
}
.cards-list{
  background: #F3FBFE;
  margin-top: 20rpx;
}
.cards-list-inp{
  height: 80rpx;
  line-height: 80rpx;
  color: #666666;
  font-size: 26rpx;
}
.cards-list-inps{
  /* display:flex;
  flex-direction: column;
  flex-wrap: wrap; */
  height: 100rpx;
  font-size: 26rpx;
}
.cards-list-inps .label{
  color: #25BDFF;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  padding-right: 10rpx;
}
.cards-list-inps .input{
  font-size: 26rpx;
  color: #25BDFF;
  height: 100rpx;
}
.cards-list-inps .input input{
  width: 100%;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #70CEFF;
} 
.bottom{
  position: fixed;
  bottom: 20rpx;
  left: 0rpx;
  z-index: 111111;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}
.quxiao{
  float: left;
  width: 50%;
  height: 100rpx ;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #FF4E50;
}
.queding{
  margin-left: 5%;
  width: 90%;
  height: 60rpx ;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #30B9EF;
 
}