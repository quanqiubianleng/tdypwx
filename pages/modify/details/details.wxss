/* pages/modify/details/details.wxss */
page{
  width: 100%;
  height: 100%;
  background: #F5F5F5;
}

.list{

  height: auto;
}
.lists{
  height: auto;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.basic{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
  font-weight: 600;
  background:#EAF7FF
}
.basics{
  background:#F5F5F5
}
.card{
  padding: 20rpx 50rpx;
  background: #fff;
  border-radius: 15rpx;
  margin-bottom: 20rpx;
  box-shadow: 0 4px 4px 0 rgba(0,0,0,0.2);
}
.card-v{
  display: inline-block;
  height: 35rpx;
  font-size: 26rpx;
  color: #666666;
  margin-right: 50rpx;
  margin-bottom: 20rpx;

}
.card-view{
  display: inline-block;
  max-width: 300rpx;
  overflow: hidden;
  height: 35rpx;
  font-size: 26rpx;
  color: #333;
  
}
.card view image{
  position: relative;
  left: 25rpx;
  top: 9rpx ;
  width: 35rpx;
  height: 35rpx;
}
.cards{
  background: #fff;
  border-radius: 15rpx;
  padding-bottom: 120rpx;
  box-shadow: 0 4px 4px 0 rgba(0,0,0,0.2);
}
.cards-tex{
  font-size: 36rpx;
  margin-top: 30rpx;
  font-weight: 600;
  color: #999999;
}
.cards-list{
  background: #F3FBFE;
  margin-top: 20rpx;
}
.cards-list-inp{
  height: 80rpx;
  line-height: 80rpx;
  color: #666666;
  font-size: 26rpx;
}
.cards-list-inps{
  /* display:flex;
  flex-direction: column;
  flex-wrap: wrap; */
  height: 100rpx;
  font-size: 26rpx;
}
.cards-list-inps .label{
  color: #25BDFF;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  padding-right: 10rpx;
}
.cards-list-inps .input{
  font-size: 26rpx;
  color: #25BDFF;
  height: 100rpx;
}
.cards-list-inps .input input{
  width: 100%;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #70CEFF;
} 
.bottom{
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  height: 100rpx;
  margin-top: 20rpx;
}
.quxiao{
  float: left;
  width: 50%;
  height: 100rpx ;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #FF4E50;
}
.queding{
  float: right;
  width: 50%;
  height: 100rpx ;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background: #30B9EF;
 
}