/* pages/jurisdiction/index/index.wxss */
page{
  background: #F8F9FA
}
.msgGroup{
  background: white;
  margin-top: 16rpx;
}
.msgGroup .msgItem{
  width: 100%;
  /* height: 116rpx; */
  border-bottom: 1rpx solid #eee;
}
.msgItems{
  width: 100%;
  height: 116rpx;
  border-bottom: 1rpx solid #eee;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.msgGroup .msgItem:last-child{
  border: none
}
.msgGroup .msgItems:last-child{
  border: none
}
.msgItem .inBox{
  width: inherit;
  height: inherit;
  position: relative
}
.inBoxs {
  width: inherit;
  height: inherit;
  position: relative
}

.msgItem .inBox .mainIcon{
  position: absolute;
  width: 68rpx;
  height: 68rpx;
  left: 32rpx;
  top: 50%;
  transform: translateY(-34rpx)
}
 .mainIcons{
  position: absolute;
  width: 68rpx;
  border-radius: 50%;
  height: 68rpx;
  left: 0;
  top: 50%;
  transform: translateY(-34rpx)
}
.msgItem .inBox .iconOpen{
  position: absolute;
  right: 32rpx;
  top: 50%;
  transform: translateY(-50%);
}
.msgItems .inBoxs .iconOpens{
  position: absolute;
  font-size: 28rpx;
  right: 10rpx;
  color: #999999;
  top: 40%;
  transform: translateY(-50%);
}
.msgItem .inBox text{
  line-height: 116rpx;
  padding-left: 126rpx;
  font-size: 32rpx;
  font-weight: 600;
}
.inBoxs .username{
  padding-top: 24rpx;
  padding-left: 94rpx;
  font-size: 28rpx;
  font-weight: 600;
}
.inBoxs .lastmg{
  display: block;
  padding-left: 94rpx;
  font-size: 24rpx;
  color: #999999;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.add{
  margin-top: 40rpx;
  margin-bottom: 60rpx;
  background: #2FB9EF;
  border-radius: 10rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
}



.badge {
  position: absolute;
  left: 46rpx;
  top: 25%;
  transform: translateY(-50%);
  padding: 0 10rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  background: red;
  color: white;
  z-index: 99;
  border-radius: 24rpx;
}

.f-badge {
  position: absolute;
  left: 80rpx;
  top: 25%;
  transform: translateY(-50%);
  padding: 0 10rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  background: red;
  color: white;
  z-index: 99;
  border-radius: 24rpx;
}
.line-h-38{
  line-height: 38rpx;
}